<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="freecrm.com Web Application  - Test Automation Test Suite">
	<listeners>
		<listener class-name="com.freecrm.qa.common.ExtentReporter" />
	</listeners>
	<test thread-count="1" name="Free CRM App Test Cases for chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.freecrm.qa.testCases.LoginPageTest"></class>
			<class name="com.freecrm.qa.testCases.MyHomePageTest"></class>
			<class name="com.freecrm.qa.testCases.ContactListPageTest"></class>
			<class name="com.freecrm.qa.testCases.NewContactPageTest"></class> 
		</classes>
	</test>
</suite>